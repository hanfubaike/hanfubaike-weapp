@import "/style/iconfont.wxss";


.btn-back {
  position: absolute;
  left: 20rpx;
  z-index: 100;
  background: rgba(255, 255, 255, 0.7);
  color: #000;
  border-radius: 16px;
  box-shadow: 0 0 2px #aaa;
  text-align: center;
  font-size: 36rpx;
  transition: 0.3s;
}
.btn-back.black {
  background: rgba(0, 0, 0, 0.3);
  color: #fff;
}
